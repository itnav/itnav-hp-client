{
  "name": "itnav-hp-client",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run start",
    "prod": "npm run check && npm run build && npm run preview",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "npm run concurrently \"npm:check:eslint -- --fix\" \"npm:check:prettier -- --write\"",
    "check": "npm run concurrently npm:check:*",
    "check:astro": "astro check",
    "check:type": "tsc --noEmit --skipLibCheck",
    "check:eslint": "eslint \"**/*.{js,cjs,mjs,ts,cts,mts,astro}\" --max-warnings 0",
    "check:prettier": "prettier . --plugin-search-dir . --check",
    "concurrently": "concurrently --prefix-colors auto",
    "astro": "astro"
  },
  "dependencies": {
    "astro": "^2.8.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-jsdoc": "^0.4.2"
  }
}
