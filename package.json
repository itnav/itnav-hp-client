{
  "name": "itnav-hp-client",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run start",
    "prod": "npm run check && npm run build && npm run preview",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "npm run concurrently \"npm:check:eslint -- --fix\" \"npm:check:stylelint -- --fix\" \"npm:check:prettier:* -- --write\"",
    "check": "npm run concurrently npm:check:*",
    "check:astro": "astro check",
    "check:type": "tsc --noEmit --skipLibCheck",
    "check:eslint": "eslint \"**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,astro}\" --max-warnings 0",
    "check:stylelint": "stylelint \"**/*.{css,scss,sass,svg,astro}\" --max-warnings 0",
    "check:prettier:standard": "prettier . --check",
    "check:prettier:astro": "prettier \"**/*.astro\" --check",
    "concurrently": "concurrently --prefix-colors auto",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/image": "^0.17.3",
    "@astrojs/sitemap": "^2.0.2",
    "@astrojs/solid-js": "^2.2.0",
    "@fontsource-variable/murecho": "^5.0.7",
    "@fontsource-variable/roboto-mono": "^5.0.9",
    "@swup/plugin": "^3.0.0",
    "@swup/preload-plugin": "^3.2.2",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "astro": "^2.10.9",
    "astro-compress": "^2.0.12",
    "astro-critters": "^2.0.10",
    "astro-purgecss": "^2.3.0",
    "astro-seo": "^0.7.5",
    "sass": "^1.66.0",
    "sharp": "^0.32.5",
    "solid-js": "^1.7.11",
    "swup": "^4.3.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-astro": "^0.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-solid": "^0.12.1",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "prettier": "^3.0.2",
    "prettier-plugin-astro": "^0.11.1",
    "prettier-plugin-jsdoc": "^1.0.1",
    "stylelint": "^15.10.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0"
  }
}
