{
  "name": "itnav-hp-client",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run start",
    "prod": "npm run check && npm run build && npm run preview",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "npm run concurrently \"npm:check:eslint -- --fix\" \"npm:check:stylelint -- --fix\" \"npm:check:prettier -- --write\"",
    "check": "npm run concurrently npm:check:*",
    "check:astro": "astro check",
    "check:type": "tsc --noEmit --skipLibCheck",
    "check:eslint": "eslint \"**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,astro}\" --max-warnings 0",
    "check:stylelint": "stylelint \"**/*.{css,scss,sass,svg,astro}\" --max-warnings 0",
    "check:prettier": "prettier . --plugin-search-dir . --check",
    "concurrently": "concurrently --prefix-colors auto",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/image": "^0.17.2",
    "@astrojs/sitemap": "^1.3.3",
    "@astrojs/solid-js": "^2.2.0",
    "@fontsource-variable/murecho": "^5.0.5",
    "@fontsource-variable/roboto-mono": "^5.0.6",
    "@swup/plugin": "^2.0.3",
    "@swup/preload-plugin": "^2.3.0",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "astro": "^2.8.0",
    "astro-compress": "^1.1.49",
    "astro-critters": "^1.1.39",
    "astro-purgecss": "^2.2.0",
    "astro-seo": "^0.7.5",
    "sass": "^1.63.6",
    "sharp": "^0.32.1",
    "solid-js": "^1.7.7",
    "swup": "^3.1.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-solid": "^0.12.1",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "stylelint": "^15.10.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0"
  }
}
