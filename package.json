{
  "name": "itnav-hp-client",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run start",
    "prod": "npm run check && npm run build && npm run preview",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "npm run concurrently \"npm:check:prettier -- --write\"",
    "check": "npm run concurrently npm:check:*",
    "check:astro": "astro check",
    "check:type": "tsc --noEmit --skipLibCheck",
    "check:prettier": "prettier . --plugin-search-dir . --check",
    "concurrently": "concurrently --prefix-colors auto",
    "astro": "astro"
  },
  "dependencies": {
    "astro": "^2.8.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "concurrently": "^8.2.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-jsdoc": "^0.4.2"
  }
}
